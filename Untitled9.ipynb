   "source": [
    "def in7(a):\n",
    "    n = ''\n",
    "    k = ''\n",
    "    while a > 0:\n",
    "        n = n + str(a % 7)\n",
    "        a = a // 7\n",
    "    n =  list(reversed(n))\n",
    "    for j in range(len(n)):\n",
    "        k += n[j]\n",
    "    return k "
   ]
